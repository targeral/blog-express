<div class="container paging">
    <div class="row">
        <div class="col-sm-12">
            <p class="btn btn-default"><%= page%></p>
            <% if(!isFirstPage) { %>
            <span class="prepage"><a href="?p=<%= page-1 %>" title="上一页" class="btn btn-default">上一页</a></span>
            <% } %>
            <% if(!isLastPage) { %>
            <span class="nextpage"><a href="?p=<%= page+1 %>" title="下一页" class="btn btn-default">下一页</a></span>
            <% } %>
        </div>
    </div>
</div>