<%-include header%>
<ul class="archive">
    <% var lastYear = 0 %>
    <% blogs.forEach(function(blog, index) {%>
        <%if(lastYear != blog.time.year) {%>
        <li><h3><%= blog.time.year %></h3></li>
        <% lastYear = blog.time.year }%>
        <li><time><%= blog.time.day %></time></li>
        <li><a href=""><%= blog.title%></a></li>
    <% }) %>
</ul>
<%-include footer%>